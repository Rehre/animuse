{
  "name": "animuse",
  "productName": "Animuse",
  "build": {
    "appId": "com.akmal.animuse",
    "win": {
      "target": "nsis"
    }
  },
  "fileAssociations": [
    {
      "ext": [
        ".mp3"
      ],
      "description": "Music file extension"
    }
  ],
  "version": "0.0.2",
  "description": "Music Player",
  "main": "src/MainManager.js",
  "scripts": {
    "start": "set NODE_ENV=production && electron .",
    "dev": "electron .",
    "dev:server": "webpack-dev-server --config webpack.dev.config.js",
    "build:react": "webpack",
    "dist": "set NODE_ENV=production && electron-builder"
  },
  "keywords": [
    "Music",
    "Player",
    "ElectronJS",
    "React"
  ],
  "author": "Akmal Maulana Azzam",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.11",
    "electron": "^2.0.3",
    "electron-builder": "^20.19.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "optimize-css-assets-webpack-plugin": "^4.0.3",
    "prop-types": "^15.6.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.6",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "btoa": "^1.2.1",
    "electron-is-dev": "^0.3.0",
    "immutability-helper": "^2.7.1",
    "jpeg-js": "^0.3.4",
    "jsmediatags": "^3.8.1",
    "mp3-duration": "^1.1.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-router-dom": "^4.3.1"
  }
}
